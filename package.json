{
  "name": "nestjs-max",
  "version": "0.1.3",
  "description": "Модуль NestJS для интеграции с Max Bot API",
  "homepage": "https://github.com/xTCry/nestjs-max#readme",
  "bugs": {
    "url": "https://github.com/xTCry/nestjs-max/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xTCry/nestjs-max.git"
  },
  "license": "MIT",
  "author": {
    "name": "xTCry (Vladislav Kh)",
    "url": "https://github.com/xTCry"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "format:sample": "prettier --config .prettierrc --write \"sample/*/src/**/*.ts\"",
    "copy-to-sample": "node scripts/copy-to-sample.js",
    "postbuild": "npm run copy-to-sample",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@maxhub/max-bot-api": "^0.2.1",
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@maxhub/max-bot-api": "^0.2.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@release-it/conventional-changelog": "^10.0.4",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "release-it": "^19.1.0",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "nest",
    "nestjs",
    "nestjs-module",
    "max",
    "max bot",
    "max bot api",
    "bot",
    "bot api",
    "bot framework"
  ]
}
